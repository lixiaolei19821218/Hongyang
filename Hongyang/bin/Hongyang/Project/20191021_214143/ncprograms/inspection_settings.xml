<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<settings version="1">

  <!--Working units - millimetres/inches-->

  <units>millimetres</units>

  <projection>
    <!--Distance to look for surfaces around measured point.-->
    <proximity_criteria>4.0</proximity_criteria>
  </projection>

  <confetti>
    <!--Size the confetti is drawn on the CAD model-->
    <diameter>8.0</diameter>
  </confetti>

  <data>
    <!--Settings for data presentation-->
    <precision>
      <!--How many decimal points to show-->
      <linear_digits>3</linear_digits>
      <angle_digits>3</angle_digits>
      <unitless_digits>5</unitless_digits>
    </precision>
    <labels>
      <!--labels will go before the point number in the report 
e.g Setting label to 'P' will display P1, P2 etc.-->
      <surface_point_label>SP</surface_point_label>
    </labels>
  </data>

</settings>
